<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>fugu.graphql</AssemblyName>
    <RootNamespace>fugu.graphql</RootNamespace>
    <Copyright>Pekka Heikura</Copyright>
    <PackageProjectUrl>https://github.com/pekkah/fugu-graphql</PackageProjectUrl>
    <RepositoryUrl>https://github.com/pekkah/fugu-graphql</RepositoryUrl>
    <Description>GraphQL Query, Mutation and Subscription library</Description>
    <PackageLicenseUrl>https://github.com/pekkah/fugu-graphql/blob/develop/LICENSE</PackageLicenseUrl>
    <PackageTags>graphql,fugu-fw</PackageTags>
    <Authors>Pekka Heikura</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GraphQL-Parser" Version="3.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="2.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="System.Threading.Channels" Version="4.5.0" />
    <PackageReference Include="System.Threading.Tasks.Dataflow" Version="4.9.0" />
  </ItemGroup>

</Project>
